import random
def guess(x):
    random_number=random.randint(1,x)
    guess=0
    
    while (x!=0):
        number=int(input("Enter ur guess:"))
        if (number==random_number):
            print("Yay ! Your guess is correct . Congrats")
        if(number!=random_number):
            print("Your guess is wrong . Try again")
            if(number>random_number):
                print("Your too high")
            else:
                print("Your too low")
print(guess(100))
